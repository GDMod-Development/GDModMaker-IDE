{
  "name": "gdmodmaker",
  "version": "0.0.1",
  "description": "An Open-Source mod creation software for Geometry Dash.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build -w"
  },
  "repository": "https://github.com/GDMod-Development/GDModMaker-IDE",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "KontrollFreek <kontrollfreekwastaken@gmail.com>",
  "license": "CC0-1.0",
  "build": {
    "appId": "com.gdmodmaker.app",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    },
    "portable": {
      "unicode": false,
      "artifactName": "GDModMaker.exe"
    }
  },
  "devDependencies": {
    "electron": "^19.0.10",
    "electron-builder": "^23.3.3"
  }
}
